{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "sourceMap": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "removeComments": true,
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
    },
    "exclude": [
      "node_modules"
    ],
    "include": ["src/**/*"]
  }